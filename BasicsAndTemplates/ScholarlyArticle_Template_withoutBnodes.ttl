### Scholarly Article Template without blank nodes

<https://doi.org/DOI_ScholarlyArticle> a schema:ScholarlyArticle ; # DOI of the conference paper
		schema:headline "" ; # Text, title of the paper
		schema:inLanguage "" ; # Language identifyer
		schema:keywords "" ; # Text, List of keywords, separated by commas
		schema:description "" ; # Text, abstract of the paper
		schema:author <https://orcid.org/OrcidOfThePerson> ; # ORCID of the author(s)
		schema:isPartOf <https://doi.org/DOIofConfProceedings> ; # DOI of conference proceedings
		schema:pageStart "" ; # Text or Integer first page
		schema:pageEnd "" ; # Text or integer last page
		schema:citation <https://doi.org/DOIofCitedPaper> ; # DOI of cited paper(s)
		schema:about cpe:ConfAbbrYear_ArtNo_RO , cpe:ConfAbbrYear_ArtNo_RM , cpe:ConfAbbrYear_ArtNo_RF , cpe:ConfAbbrYear_ArtNo_FW , cpe:ConfAbbrYear_ArtNo_Cr .
		
		# Research Objectives
		cpe:ConfAbbrYear_ArtNo_RO a schema:ItemList ;
			schema:name "Research Objectives" ;
			schema:itemListElement cpe:ConfAbbrYear_ArtNo_RO1 , cpe:ConfAbbrYear_ArtNo_RO2 .
		cpe:ConfAbbrYear_ArtNo_RO1 a schema:StructuredValue ; 
				schema:description "" ;				
				cpv:researchActionType cpv:ResActType1 ; # NamedEntity Research Activity Type 1
				cpv:researchArtifact cpv:ResArtifact1 . # NamedEntity Research Artifact 1 
		cpe:ConfAbbrYear_ArtNo_RO2 a schema:StructuredValue ;
				schema:description "" ;
				cpv:researchActionType cpv:ResActType2 ; # NamedEntity Research Activity Type 2
				cpv:researchArtifact cpv:ResArtifact2 . # NamedEntity Research Artifact 2 
		# proceed if necessary
		
		# Research Methods
		cpe:ConfAbbrYear_ArtNo_RM a schema:ItemList ;
			schema:name "Research Methods" ;
			schema:itemListElement cpe:ConfAbbrYear_ArtNo_RM1 , cpe:ConfAbbrYear_ArtNo_RM2 .
		cpe:ConfAbbrYear_ArtNo_RM1 a schema:StructuredValue ;
				schema:description "" ; 
				cpv:researchMethod cpv:ResMeth1 .	# NamedEntity Research Method 1		
		cpe:ConfAbbrYear_ArtNo_RM2 a schema:StructuredValue ;
				schema:description "" ; 
				cpv:researchMethod cpv:ResMeth2 .	# NamedEntity Research Method 2	
		# proceed if necessary
			
		# Research Findings
		cpe:ConfAbbrYear_ArtNo_RF a schema:ItemList ;
			schema:name "Research Findings" ;
			schema:itemListElement "" , "" . # Text Research Findings 1 and 2 etc.
			
		# Future Work
		cpe:ConfAbbrYear_ArtNo_FW a schema:ItemList ;
			schema:name "Future Work" ;
			schema:itemListElement "" , ""  . # Text Future Work 1 and 2 etc.

		# Critique
		cpe:ConfAbbrYear_ArtNo_Cr a schema:ItemList ;
			schema:name "Critique" ;
			schema:itemListElement "" , "" . # Text Critique 1 and 2 etc.
			
