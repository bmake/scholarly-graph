<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:ns0="https://schema.org/">

  <owl:Class rdf:about="https://schema.org/CreativeWork">
    <rdfs:label>Creative Work</rdfs:label>
    <rdfs:comment>Here used primarily for scientific papers, conference proceeding and related creative work.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/ScholarlyArticle">
    <rdfs:label>Scholarly Article</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:comment>The class of scientific papers.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/CreativeWorkSeries">
    <rdfs:label>Creative Work Series</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:comment>The class of Conference proceedings - independent of the publication format.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Event">
    <rdfs:label>Event</rdfs:label>
    <rdfs:comment>Here used primarily for scientific conferences.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/PublicationEvent">
    <rdfs:label>Publication Event</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://schema.org/Event"/>
    <rdfs:comment>The class of scientific conferences.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Organization">
    <rdfs:label>Organization</rdfs:label>
    <rdfs:comment>Here used for two objectives: (i) to denote the affiliations of authors and other Person and (ii) for indicate publishers and conference organizers.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Place">
    <rdfs:label>Place</rdfs:label>
    <rdfs:comment>Here used for specifying the locations of conferences and organizations.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Person">
    <rdfs:label>Person</rdfs:label>
    <rdfs:comment>Mainly used for identifying authors, editors, attendees, contributers, organizers and other human agents.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Intangible">
    <rdfs:label>Inangible</rdfs:label>
    <rdfs:comment>A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc. (taken from schema.org).</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Enumeration">
    <rdfs:label>Enumeration</rdfs:label>
    <rdfs:comment>Lists or enumerations of aspects specifying a scientific paper, like research methods, research artifacts and research action types.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/StructuredValue">
    <rdfs:label>Structured Value</rdfs:label>
    <rdfs:comment>Structured values are used when the value of a property has a more complex structure than simply being a textual value or a reference to another thing. Here used for specifying research objectives characterized by research actions applied to research artifacts.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/ItemList">
    <rdfs:label>Item List</rdfs:label>
    <rdfs:comment>Lists of enumerations of aspects specifying a scientific paper, like research methods, research findings, research objectives, and future work.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
  </owl:Class>

  <owl:Class rdf:about="https://bmake.th-brandenburg.de/spv#ResearchMethod">
    <rdfs:label>Research Method</rdfs:label>
    <rdfs:comment>List of all relevant research methods used in Information Systems.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Enumeration"/>
  </owl:Class>

  <owl:Class rdf:about="https://bmake.th-brandenburg.de/spv#ResearchArtifact">
    <rdfs:label>Research Artifact</rdfs:label>
    <rdfs:comment>Classification of all types of research &amp; development artifacts characteristic for Information Systems.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Enumeration"/>
  </owl:Class>

  <owl:Class rdf:about="https://bmake.th-brandenburg.de/spv#ResearchActionType">
    <rdfs:label>Research Action Type</rdfs:label>
    <rdfs:comment>Classification of all types of research &amp; development actions applied in Information Systems.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Enumeration"/>
  </owl:Class>

  <owl:Class rdf:about="https://schema.org/Language">
    <rdfs:label>Language</rdfs:label>
    <rdfs:comment>Natural languages such as Spanish, Tamil, Hindi, English, etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
  </owl:Class>

  <rdfs:Class rdf:about="https://schema.org/DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Encompasses basic data types for typing literals. </rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="https://schema.org/Text">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>General class for all literals being of any textual Datatype.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/DataType"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="https://schema.org/URL">
    <rdfs:label>URL</rdfs:label>
    <rdfs:comment>General class for all literals being an URL.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/Text"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="https://schema.org/DateTime">
    <rdfs:label>Date and Time</rdfs:label>
    <rdfs:comment>General class for all literals indicating a data and a time.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/DataType"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="https://schema.org/Date">
    <rdfs:label>Date</rdfs:label>
    <rdfs:comment>General class for all literals indicating a data.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://schema.org/DataType"/>
  </rdfs:Class>

  <owl:ObjectProperty rdf:about="https://schema.org/about">
    <rdfs:label>about</rdfs:label>
    <rdfs:comment>Is used to relate to subject matters of a creative work, mainly a scientific paper. Here mainly used to relate to blank nodes of type schema:ItemList for specifying characteristics of a scientific paper, like research findings, research methods and research objectives.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/citation">
    <rdfs:label>citation</rdfs:label>
    <rdfs:comment>Is used to create references of a scientific paper to other items of creative work (particularly other papers).</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/isPartOf">
    <rdfs:label>is part of</rdfs:label>
    <rdfs:comment>Is used to indicate the proceedings where a scientific paper is published.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="https://schema.org/CreativeWorkSeries"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/author">
    <rdfs:label>author</rdfs:label>
    <rdfs:comment>Is used to indicate a author of a scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="https://schema.org/Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/affiliation">
    <rdfs:label>affiliation</rdfs:label>
    <rdfs:comment>Is used to indicate the affiliation of a person to an organization.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Organization"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/editor">
    <rdfs:label>editor</rdfs:label>
    <rdfs:comment>Is used to indicate an editor of a scientific paper or conference proceedings.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="https://schema.org/Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/publisher">
    <rdfs:label>publisher</rdfs:label>
    <rdfs:comment>Is used to indicate a publishing organization of a scientific paper or conference proceedings.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="https://schema.org/Organization"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/attendee">
    <rdfs:label>attendee</rdfs:label>
    <rdfs:comment>Is used to indicate a person attending a conference.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/PublicationEvent"/>
    <rdfs:range rdf:resource="https://schema.org/Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/contributor">
    <rdfs:label>contributor</rdfs:label>
    <rdfs:comment>Is used to indicate a person contributing to a conference.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/PublicationEvent"/>
    <rdfs:range rdf:resource="https://schema.org/Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/organizer">
    <rdfs:label>organizer</rdfs:label>
    <rdfs:comment>Is used to indicate a person or an organization organizing a conference.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/PublicationEvent"/>
    <rdfs:range>
      <rdf:Description>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="https://schema.org/Person"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="https://schema.org/Organization"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </rdf:Description>
    </rdfs:range>

  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/recordedIn">
    <rdfs:label>recorded in</rdfs:label>
    <rdfs:comment>Is used to relate a conference to the corresponding proceedings.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/PublicationEvent"/>
    <rdfs:range rdf:resource="https://schema.org/CreativeWorkSeries"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/location">
    <rdfs:label>location</rdfs:label>
    <rdfs:comment>Is used for different purposes to referring to a geographical place, e. g. for event locations (termporal) or for organization locations (permanent).</rdfs:comment>
    <rdfs:domain>
      <rdf:Description>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="https://schema.org/Organization"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="https://schema.org/Event"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </rdf:Description>
    </rdfs:domain>

    <rdfs:range rdf:resource="https://schema.org/Place"/>
  </owl:ObjectProperty>

  <rdf:Property rdf:about="https://schema.org/itemListElement">
    <rdfs:label>item list element</rdfs:label>
    <rdfs:comment>For itemListElement values simple strings or existing entities can be used.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ItemList"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:range>

  </rdf:Property>

  <owl:ObjectProperty rdf:about="https://bmake.th-brandenburg.de/spv#researchArtifact">
    <rdfs:label>research artifact</rdfs:label>
    <rdfs:comment>Is used in combination with research action type to indicate a research objective of a scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/StructuredValue"/>
    <rdfs:range rdf:resource="https://bmake.th-brandenburg.de/spv#ResearchArtifact"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://bmake.th-brandenburg.de/spv#researchActionType">
    <rdfs:label>research action type</rdfs:label>
    <rdfs:comment>Is used in combination with research action type to indicate a research objective of a scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/StructuredValue"/>
    <rdfs:range rdf:resource="https://bmake.th-brandenburg.de/spv#ResearchActionType"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://bmake.th-brandenburg.de/spv#researchMethod">
    <rdfs:label>research method</rdfs:label>
    <rdfs:comment>Is used to indicate a research method described in a scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/StructuredValue"/>
    <rdfs:range rdf:resource="https://bmake.th-brandenburg.de/spv#ResearchMethod"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/description">
    <rdfs:label>description</rdfs:label>
    <rdfs:comment>General datatype property, potentially usable for description of all resources.</rdfs:comment>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/image">
    <rdfs:label>image</rdfs:label>
    <rdfs:comment>General datatype property, potentially usable for relating an image of a resource.</rdfs:comment>
    <rdfs:range rdf:resource="https://schema.org/URL"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/sameAs">
    <rdfs:label>same as</rdfs:label>
    <rdfs:comment>General datatype property, potentially usable for relating one resource to a URL representing the same resource.</rdfs:comment>
    <rdfs:range rdf:resource="https://schema.org/URL"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/name">
    <rdfs:label>name</rdfs:label>
    <rdfs:comment>General datatype property, potentially usable for specifying the name of a resource.</rdfs:comment>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/alternateName">
    <rdfs:label>alternative name</rdfs:label>
    <rdfs:comment>General datatype property, potentially usable for specifying alternative names of a resource.</rdfs:comment>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/url">
    <rdfs:label>url</rdfs:label>
    <rdfs:comment>General datatype property, potentially usable for specifying the URL of a resource.</rdfs:comment>
    <rdfs:range rdf:resource="https://schema.org/URL"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/endTime">
    <rdfs:label>end time</rdfs:label>
    <rdfs:comment>The end time of some event.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Event"/>
    <rdfs:range rdf:resource="https://schema.org/DateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/startTime">
    <rdfs:label>start time</rdfs:label>
    <rdfs:comment>The start time of some event.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Event"/>
    <rdfs:range rdf:resource="https://schema.org/DateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/headline">
    <rdfs:label>headline</rdfs:label>
    <rdfs:comment>Headline of a scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/alternativeHeadline">
    <rdfs:label>alternative headline</rdfs:label>
    <rdfs:comment>A secondary title of a scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/inLanguage">
    <rdfs:label>in language</rdfs:label>
    <rdfs:comment>The language of the content or performance or used in an event.</rdfs:comment>
    <rdfs:domain>
      <rdf:Description>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="https://schema.org/Event"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="https://schema.org/CreativeWork"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </rdf:Description>
    </rdfs:domain>

    <rdfs:range rdf:resource="https://schema.org/Language"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/dateCreated">
    <rdfs:label>date created</rdfs:label>
    <rdfs:comment>The date on which the CreativeWork was created or the item was added to a DataFeed.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="https://schema.org/Date"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/dateModified">
    <rdfs:label>date modified</rdfs:label>
    <rdfs:comment>The date on which the CreativeWork was modified or the item's entry was modified within a DataFeed.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="https://schema.org/Date"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/pageEnd">
    <rdfs:label>end page</rdfs:label>
    <rdfs:comment>The page on which the scientific paper ends.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/pageStart">
    <rdfs:label>start page</rdfs:label>
    <rdfs:comment>The page on which the scientific paper starts.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/keywords">
    <rdfs:label>keywords</rdfs:label>
    <rdfs:comment>Keywords or tags used to describe the content of a creative work. Multiple entries in a keywords list are typically delimited by commas.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/text">
    <rdfs:label>text</rdfs:label>
    <rdfs:comment>The textual content of this creative work.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/CreativeWork"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/award">
    <rdfs:label>award</rdfs:label>
    <rdfs:comment>An award won by or for this scientific paper.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/ScholarlyArticle"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/familyName">
    <rdfs:label>family name</rdfs:label>
    <rdfs:comment>The last name of a Person.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/givenName">
    <rdfs:label>given name</rdfs:label>
    <rdfs:comment>The first name of a Person.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/additionalName">
    <rdfs:label>additional name</rdfs:label>
    <rdfs:comment>The additional name for Person, can be used for a middle name.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/honoricPrefix">
    <rdfs:label>honoric prefix</rdfs:label>
    <rdfs:comment>An honorific prefix preceding a Person's name such as Dr/Mrs/Mr.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/honoricSuffix">
    <rdfs:label>honoric suffix</rdfs:label>
    <rdfs:comment>An honorific suffix preceding a Person's name such as M.D. /PhD/MSCSW.</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/jobTitle">
    <rdfs:label>job title</rdfs:label>
    <rdfs:comment>The job title of the person (for example, Knowledge Engineer).</rdfs:comment>
    <rdfs:domain rdf:resource="https://schema.org/Person"/>
    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/email">
    <rdfs:label>email</rdfs:label>
    <rdfs:comment>Email address of a person or an organization.</rdfs:comment>
    <rdfs:domain>
      <rdf:Description>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="https://schema.org/Person"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="https://schema.org/Organization"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </rdf:Description>
    </rdfs:domain>

    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/telephone">
    <rdfs:label>telephone</rdfs:label>
    <rdfs:comment>Telephone number of a person or an organization.</rdfs:comment>
    <rdfs:domain>
      <rdf:Description>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="https://schema.org/Person"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="https://schema.org/Organization"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </rdf:Description>
    </rdfs:domain>

    <rdfs:range rdf:resource="https://schema.org/Text"/>
  </owl:DatatypeProperty>

